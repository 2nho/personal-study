<title>Insert title here</title>
</head>
<body>
	<h3>data1 : <%=request.getAttribute("data1") %></h3>
	<h3>data1 : ${data1}</h3>
	<h3>data2 : <%=request.getAttribute("data2") %></h3>
	<h3>data2 : ${data2}</h3>
	<h3>vo1.data1 : <%=((DataVO)request.getAttribute("vo1")).getData1() %></h3>
</body>
